  setDeleteUser(false);\n    } else if (!loading) {\n    }\n  }, [userData, loading, dispatch, delUserState]);\n\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    if (!searchInput) {\n      return false;\n    }\n\n    try {\n\n      if (!users) {\n        throw new Error(\"Unable to Find any User\");\n      }\n\n      const response = users.filter((user) => {\n        if (user._id === searchInput) {\n          try {\n            async function delUser() {\n              const mutationResponse = await deleteUser({\n                variables: {\n                  _id: user._id\n                },\n              });\n              if (mutationResponse) {\n                alert(\"You have successfully Deleted a User\");\n                let index = userData.users.indexOf(user._id);\n                userData.users.splice(index, 1);\n                setDeleteUser(true);\n              }\n            }\n            delUser();\n          }\n          catch (e) {\n            console.error(e);\n          }\n        }\n      });\n\n    } catch (err) {\n      console.log(err);\n    }\n  };\n\n  return (\n    <>\n      <MDBContainer>\n        <MDBRow>\n          <MDBCol md=\"6\">\n            <form onSubmit={ handleFormSubmit }>\n              <p className=\"h4 text-center mb-4\">Find a User</p>\n              <select\n                className=\"browser-default custom-select\"\n                onChange={ (e) => setSearchInput(e.target.value) }\n                type=\"searchInput\"\n                name=\"searchInput\"\n                value={ searchInput }\n              >\n                <option>Choose your option</option>\n                { userDataID.map((user) => {\n                  return <option key={ user._id } value={ user._id }>{ user.firstName }</option>;\n                }) }\n              </select>\n\n              <div className=\"text-center mt-4\">\n                <MDBBtn color=\"success\" type=\"submit\">\n                  Submit\n                </MDBBtn>\n              </div>\n            </form>\n          </MDBCol>\n        </MDBRow>\n      </MDBContainer>\n    </>\n  );\n};\n\nexport default DeleteUser;","/Users/ahmadelgamal/Documents/Sites/UCB/Projects/PROJECT 3/adopt-me/client/src/components/EditBreedAdmin/index.js",[],"/Users/ahmadelgamal/Documents/Sites/UCB/Projects/PROJECT 3/adopt-me/client/src/components/EditUser/index.js",[],"/Users/ahmadelgamal/Documents/Sites/UCB/Projects/PROJECT 3/adopt-me/client/src/components/BreedListAdmin/index.js",[],"/Users/ahmadelgamal/Documents/Sites/UCB/Projects/PROJECT 3/adopt-me/client/src/components/UserListAdmin/index.js",[],"/Users/ahmadelgamal/Documents/Sites/UCB/Projects/PROJECT 3/adopt-me/client/src/components/DogsListAdmin/index.js",[],"/Users/ahmadelgamal/Documents/Sites/UCB/Projects/PROJECT 3/adopt-me/client/src/components/AddDog/index.js",[],"/Users/ahmadelgamal/Documents/Sites/UCB/Projects/PROJECT 3/adopt-me/client/src/components/AddBreed/index.js",[],"/Users/ahmadelgamal/Documents/Sites/UCB/Projects/PROJECT 3/adopt-me/client/src/components/EditDogAdmin/index.js",[],{"ruleId":"195","replacedBy":"196"},{"ruleId":"197","replacedBy":"198"},{"ruleId":"199","severity":1,"message":"200","line":39,"column":35,"nodeType":"201","messageId":"202","endLine":39,"endColumn":37},{"ruleId":"203","severity":1,"message":"204","line":19,"column":17,"nodeType":"205","endLine":19,"endColumn":30},{"ruleId":"203","severity":1,"message":"204","line":22,"column":17,"nodeType":"205","endLine":22,"endColumn":30},{"ruleId":"195","replacedBy":"206"},{"ruleId":"197","replacedBy":"207"},{"ruleId":"208","severity":1,"message":"209","line":50,"column":13,"nodeType":"210","messageId":"211","endLine":50,"endColumn":21},{"ruleId":"199","severity":1,"message":"212","line":50,"column":44,"nodeType":"201","messageId":"202","endLine":50,"endColumn":46},"no-native-reassign",["213"],"no-negated-in-lhs",["214"],"array-callback-return","Array.prototype.map() expects a ret